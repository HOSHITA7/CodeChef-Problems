import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
        Scanner scanne = new Scanner(System.in);
        
        int x = scanne.nextInt();
        
        int y = scanne.nextInt();
        
        int res = x + y;
        
        int ans = 2 * res;
         
        if( ans < 1000) System.out.println("NO");
        
        else  System.out.println("YES");
	}
}

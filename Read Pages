import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
        
        Scanner scanne = new Scanner(System.in);
        
        int t  = scanne.nextInt();
        
        while (t --> 0) {
            
            int n = scanne.nextInt();
            
            int p = scanne.nextInt();
            
            int d = scanne.nextInt();
            
            if (n <= p * d)  System.out.println("YES");
            
            else  System.out.println("NO");
        }
	}
}

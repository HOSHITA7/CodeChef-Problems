import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
        Scanner scanne = new Scanner(System.in);
        
        int x = scanne.nextInt();
        
        int y = scanne.nextInt();
        
        int res = y + y;
        
        if (x < res)  System.out.println("No");
        
        else  System.out.println("Yes");
	}
}

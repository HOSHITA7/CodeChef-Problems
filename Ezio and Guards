import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
        
        Scanner scanne = new Scanner(System.in);
        
        int t = scanne.nextInt();
        
        while (t --> 0) {
            
         int e = scanne.nextInt();
        
         int g = scanne.nextInt();
         
         if (e >= g)   System.out.println("YES");
        
         else   System.out.println("NO");
        }
	}
}

import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
        
        Scanner scanne = new Scanner(System.in);
        
        int n = scanne.nextInt();
        
        int res = n + 1;
        
        System.out.println(res * 5000);
	}
}

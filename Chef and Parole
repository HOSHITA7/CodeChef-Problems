import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
        
        Scanner scanne = new Scanner(System.in);
        
        int d = scanne.nextInt();
        
        if (d >= 7)  System.out.println("Yes");
        
        else  System.out.println("No");
	}
}

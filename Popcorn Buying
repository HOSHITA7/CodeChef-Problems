import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
         Scanner sc = new Scanner(System.in);
         
         int x = sc.nextInt();
         
         int res = x - 100;     // int res = x - 100/ 50; System.out.println(res);
         
         if (res > 50) {
             
             res = res / 50;
             
            System.out.println(res);
         }
         else System.out.println(0);
	}
}

import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
        Scanner scanne = new Scanner(System.in);
        
        int n = scanne.nextInt();
        
        if (n < 3)  System.out.println("GOLD");
        
        else if (n >= 3 && n < 6) System.out.println("SILVER");
        
        else  System.out.println("BRONZE");
	}
}

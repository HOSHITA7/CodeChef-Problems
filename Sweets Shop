import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
        
        Scanner scanne = new Scanner(System.in);
        
        int c = scanne.nextInt();
        
        int l = scanne.nextInt();
        
        int res = l * 10;
        
        int ans = c - res;
        
        if (ans == 0)  System.out.println(0);
        
        else {
            
            int j = ans / 20;
            
            System.out.println(j);
        }
	}
}

import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
        Scanner scanne = new Scanner(System.in);
        
        int t = scanne.nextInt();
        
        while (t --> 0) {
            
            int x = scanne.nextInt();
            
            int y = scanne.nextInt();
            
            int z = scanne.nextInt();
            
            int res = y * z;
            
            if (x <= res)  System.out.println("Yes");
            
            else  System.out.println("No");
        }
	}
}

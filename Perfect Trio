import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
        
        Scanner scanne = new Scanner(System.in);
        
        int t = scanne.nextInt();
        
        while (t --> 0) {
            
            int a = scanne.nextInt();
            
            int b = scanne.nextInt();
            
            int c = scanne.nextInt();
            
            int A = b + c;
            
            int B = a + c;
            
            int C = a + b;
            
            if (A == a || B == b || C == c)   System.out.println("YES");
            
            else  System.out.println("NO");
        }
	}
}

import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
        
        Scanner scanne = new Scanner(System.in);
        
        int e = scanne.nextInt();
        
        int c = scanne.nextInt();
        
        int res = e * 2;
        
        if (c >= res)  System.out.println("YES");
        
        else System.out.println("NO");
	}
}

import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
        
        Scanner scanne = new Scanner(System.in);
        
        int t = scanne.nextInt();
        
        while (t --> 0) {
            
            int n = scanne.nextInt();
            
            int ans = 0;
            
            while (n > 0) {
                
                ans += n % 10;
                
                n /= 10;
            }
            
            System.out.println(ans);
        }
	}
}

import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
        Scanner sc = new Scanner(System.in);
        
        String s = sc.nextLine();      
        
        if(s.length() == 3 && s.charAt(0) == s.charAt(2) && s.charAt(1) == 'w') 
               System.out.println("Cute");
               
        else   System.out.println("No");
	}
}

///

import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
        Scanner sc = new Scanner(System.in);
        
        String s = sc.next();
        
    System.out.println(s.length() == 3 && s.charAt(0) == s.charAt(2) && s.charAt(1) == 'w' ? "Cute": " No");
    
	}
}

///

import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc = new Scanner(System.in);
		
        int g = sc.nextInt();
        
        int v = sc.nextInt();
        
        if (g < v)  System.out.println(0);
        
        else {
            
            int c = g - v;
            
            System.out.println(c);
        }
	}
}

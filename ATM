import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
        
        Scanner scanne = new Scanner(System.in);
        
            int x = scanne.nextInt();
            
            double y = scanne.nextDouble();
            
            if (x % 5 == 0 && x + 0.50 <= y)  y = y - x - 0.50;
            
            System.out.printf("%.2f" , y);
	}
}

import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
        Scanner scanne = new Scanner(System.in);
        
        int t = scanne.nextInt();
        
        while (t-->0) {
            
            int a = scanne.nextInt();
            
            int b = scanne.nextInt();
            
            if (a > b) System.out.println("LOSS");
            
            else if (a == b)  System.out.println("NEUTRAL");
            
            else  System.out.println("PROFIT");
        }
	}
}

import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
        
        Scanner scanne = new Scanner(System.in);
        
        int SP = scanne.nextInt();
        
        int CP = scanne.nextInt();
        
        int PP = scanne.nextInt();
        
        int res = CP + PP;
        
        if (res >= SP)  System.out.println("YES");
        
        else   System.out.println("NO");
	}
}

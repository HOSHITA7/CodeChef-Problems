class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		
		Scanner scanne = new Scanner(System.in);
        
        int n = scanne.nextInt();
       
        for (int i = 0; i < n; i++) {
            
            int[] a = new int[3];
            
            for (int j = 0; j < 3; j++) a[j] = scanne.nextInt();
            
            Arrays.sort(a);
            
            System.out.println(a[1]);
        }
	}
}

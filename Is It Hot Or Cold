import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
        Scanner scanne = new Scanner(System.in);
        
        int t = scanne.nextInt();
        
        while (t --> 0) {
            
            int x = scanne.nextInt();
            
            if (x > 20)  System.out.println("HOT");
            
            else  System.out.println("COLD");
        }
	}
}

import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
        Scanner sc = new Scanner(System.in);
        
        int x1 = sc.nextInt();
        
        int y1 = sc.nextInt();
        
        int x = sc.nextInt();
        
        int y = sc.nextInt();
        
        int res1 = x1 - x;
        
        int res2 = y1 - y;
        
        System.out.println(res1 + " " + res2);
	}
}

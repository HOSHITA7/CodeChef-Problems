import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
         Scanner scanne = new Scanner(System.in);
         
         String s = scanne.nextLine();
         
         if (s.length() == 10) {
             
           char c = s.charAt(6);
           
           System.out.println(c); 
         }
         
         else  System.out.println("Invalid input");
	}
}

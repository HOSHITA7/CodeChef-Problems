import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
        
        Scanner scanne = new Scanner(System.in);
        
        int t = scanne.nextInt();
        
        while (t --> 0) {
            
            int x = scanne.nextInt();
            
            int y = scanne.nextInt();
            
            int r1 = x - y;
            
            int r2 = x / 10;
            
            int r3 = x + r2;
            
            int ans = r3 - r1;
            
            System.out.println(ans);
        }
	}
}

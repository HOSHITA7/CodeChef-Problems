import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
      Scanner scanne = new Scanner(System.in);
      
      int N = scanne.nextInt();
      
      if (N <= 30)  System.out.println("NO");
      
      else   System.out.println("YES");
	}
}
